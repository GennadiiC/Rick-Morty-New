.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/redux/rickMortySlice.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/NavBar.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/Likes.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/Episodes.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/reusable/PaginationFilter.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/reusable/FilterForm.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/reusable/Character.js",["146"],[],"import { useEffect, useState } from \"react\";\n\nexport const Character = ({ \n  name, \n  species, \n  gender,  \n  location, \n  status,\n  episode,\n  created, \n  image\n}) => {\n\n  \n  const [ clicked, setClicked ] = useState(false)\n  const [ episodes, setEpisodes ] = useState(null)\n  const [ error, setError ] = useState(null)\n\n\n  let idsArr = episode.map(ep => Number(ep.slice(-2).replace('/', '')))\n  let ids = idsArr.join(',')\n\n  const fetchEpisodes = async () => {\n    try {\n      const response = await fetch(\n        `https://rickandmortyapi.com/api/episode/${ids}`\n      )\n      let result = await response.json()\n      setEpisodes(result)\n    } catch (err) {\n      setError(err.message)\n    }\n  }\n\n  useEffect(() => {\n    fetchEpisodes()\n  }, [])\n\n\n  function episodesData (data) {\n    if (Array.isArray(episodes)) {\n      return episodes.map(ep => ep[data]).join(', ') \n    } else {\n      return episodes[data]\n    }\n  }\n\n\n  let title = episodes ? episodesData('name') : error ? error : null\n\n  let series = episodes ? episodesData('episode') : error ? error : null\n\n\n  return ( \n    <div className=\"d-flex justify-content-between my-2 rounded shadow\">\n      \n      <div className=\"container\">\n        <img className=\"float-start rounded img-fluid my-3 me-3\" src={image} alt={name} />\n        <div className=\"mt-3\">\n          <h5>Name: <span className=\"sm\">{name}</span>, Status: <span className=\"sm\">{status}</span></h5> \n          <div className=\"d-flex flex-column align-items-end\">\n            <button className=\"btn my-3\" onClick={() => setClicked(!clicked)}>{!clicked ? 'Show Info' : 'Hide Info'}</button>\n          </div> \n          <div className={!clicked ? \"d-none\" : null}>\n            <p><span className=\"fs-5\">Species:</span> {species}</p>\n            <p><span className=\"fs-5\">Gender:</span> {gender}</p>\n            <p><span className=\"fs-5\">Location:</span> {location.name}</p>\n            <p><span className=\"fs-5\">Created:</span> {created.slice(0, -14)}</p>\n            <div>\n              <p className=\"fs-5\">Episode(s):</p>\n              <div>\n                <p className=\"ser\">series:</p> \n                <p>{series}</p>\n              </div>\n              <div>\n                <p className=\"ser\">title:</p> \n                <p>{title}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}","/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/Locations.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/reusable/Table.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/CharacterList.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/Searchform.js",[],[],"/Users/g/Documents/myCodeProjects/rick-morty-new/src/components/WatchList.js",["147"],[],"import { useForm } from 'react-hook-form';\nimport { useEffect } from 'react';\nimport { getAllEpisodesAsync, setSearchFormEpisode, setWatchList } from '../redux/rickMortySlice';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { Table } from './reusable/Table'\n\n\n// component to display on 'My Watchlist' link\nexport const WatchList = () => {\n\n  const { register, handleSubmit, reset } = useForm();\n\n  const dispatch = useDispatch()\n\n  useEffect(() => {\n    dispatch(getAllEpisodesAsync())\n  }, [dispatch])\n\n\n  const {\n    displayAllEpisodes,\n    allEpisodesStatus,\n    allEpisodesError, \n    searchFormEpisode,\n    watchlist\n  } = useSelector(state => state.rickMorty)\n\n  useEffect(() => {\n    if (localStorage.length) {\n      let arr = []\n      for (let i = 0; i < localStorage.length; i++) {\n        let obj = JSON.parse(localStorage.getItem(localStorage.key(i)))\n        arr.push(obj)\n      }\n      dispatch(setWatchList(arr))\n    }\n  }, [localStorage.length])\n\n\n  const onSubmit = (value) => {\n    if (value.episode !== '') {\n      dispatch(setSearchFormEpisode(value.episode))\n      let foundEpisode = displayAllEpisodes.find(ep => ep.name === value.episode)\n      foundEpisode = {\n        ...foundEpisode,\n        checked: false\n      }\n      if (JSON.parse(localStorage.getItem(foundEpisode.id)) !== foundEpisode.id) {\n        localStorage.setItem(foundEpisode.id, JSON.stringify(foundEpisode))\n      }\n      reset();\n    }\n  }\n\n\n  return (\n    <div className='container-fluid mt-5 custom-width'>\n      <h3 className='h3 text-center my-4'>Watchlist</h3>\n    \n      {\n        searchFormEpisode === '' ?\n        <p className=\"text-center my-3\">Enter name...</p> :\n        allEpisodesStatus === 'loading' ?\n        <p className=\"text-center my-3\">Loading...</p> :\n        allEpisodesStatus === 'succeeded' && displayAllEpisodes.error ?\n        <p className=\"text-center my-3\">Ops, {displayAllEpisodes.error.toLowerCase()}!</p> :\n        allEpisodesStatus === 'rejected' && allEpisodesError ?\n        <p className=\"text-center my-3\">Ops, {allEpisodesError.toLowerCase()}!</p> :\n        null\n      }\n\n      <form className='input-group input-group-sm my-4' onSubmit={handleSubmit(onSubmit)}>\n        <input \n          className=\"form-control\" \n          list=\"datalistOptions\" \n          id=\"exampleDataList\" \n          {...register('episode')} \n          aria-label=\"Text input with dropdown button\" \n          autoComplete=\"off\" \n        />\n        <datalist className='datalist' id=\"datalistOptions\">\n          \n          { \n            allEpisodesStatus === 'loading' ?\n            <option label='loading'>Loading...</option> :\n            allEpisodesStatus === 'succeeded' && displayAllEpisodes ?\n            displayAllEpisodes.map(ep => \n              <option key={ep.id} value={ep.name} />\n            ) :\n            allEpisodesStatus === 'failed' && allEpisodesError ?\n            <option label='error'>Oops, {allEpisodesError}</option> :\n            null\n          }\n          \n        </datalist>\n        <button className=\"btn\" type=\"button\" onClick={handleSubmit(onSubmit)}>Add Episode</button>\n      </form>\n\n      {\n        watchlist === null ?\n        <p className=\"text-center my-3\">Your watchlist is empty.</p> :\n        watchlist !== null ? \n        <p className=\"text-center my-3\">Your watchlist:</p> :\n        null\n      }\n      {\n        watchlist !== null ?\n          <Table \n            dataArr={watchlist}\n            className={'table table-hover'}\n            col1={'Name'}\n            col2={'Code'}\n            col3={'Air Date'}\n            col4={'Remove From Watchlist'}\n            col5={'Done'}\n            mapProps={{\n              one: 'name',\n              two: 'episode',\n              three: 'air_date',\n              four: 'click to remove'\n            }}\n          />\n         :\n        null\n      }\n    </div>\n  )\n}",{"ruleId":"148","severity":1,"message":"149","line":37,"column":6,"nodeType":"150","endLine":37,"endColumn":8,"suggestions":"151"},{"ruleId":"148","severity":1,"message":"152","line":37,"column":6,"nodeType":"150","endLine":37,"endColumn":27,"suggestions":"153"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchEpisodes'. Either include it or remove the dependency array.